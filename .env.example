# Supabase Configuration
# Copy this file to .env and fill in your Supabase project details

# Your Supabase project URL (e.g., https://your-project.supabase.co)
EXPO_PUBLIC_SUPABASE_URL=

# Your Supabase anon/public key
EXPO_PUBLIC_SUPABASE_ANON_KEY=

# Optional: Local development settings
# Set to "true" to use local Supabase instance
EXPO_PUBLIC_USE_SUPABASE_EMULATOR=false

# Optional: Custom emulator host (default: 127.0.0.1 for web, 10.0.2.2 for native)
EXPO_PUBLIC_SUPABASE_EMULATOR_HOST=127.0.0.1

# Optional: Custom emulator port (default: 54321)
EXPO_PUBLIC_SUPABASE_EMULATOR_PORT=54321